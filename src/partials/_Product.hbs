                    </picture>
                    <picture>
                        <source srcset='https://statics.venca.com/web/img/products/112004_262073_PV_DF_BG_20230214101733_5_xs.jpg'>
                        <img src='https://statics.venca.com/web/img/products/112004_262073_PV_DF_BG_20230214101733_5_xs.webp' class='thumbnail img-fluid' />
                    </picture>
                    <picture>
                        <source srcset='https://statics.venca.com/web/img/products/112004_262073_PV_DF_DT_20230214101733_6_xs.jpg'>
                        <img src='https://statics.venca.com/web/img/products/112004_262073_PV_DF_DT_20230214101733_6_xs.webp' class='thumbnail img-fluid' />
                    </picture>                
                </div>
                <!-- Main -->
                <div id="mainImage" class="col-md-12 col-lg-10">
                    <a class="d-block" data-bs-toggle="modal" data-bs-target="#zoomImg">
                        <img src='https://statics.venca.com/web/img/products/112004_262073_PV_DF_FR_20230214101733_1_l.webp' width='591' height='791' class='w-100 img-fluid' loading='lazy' title='' alt='' />
                    </a>
                    <!-- Modal Zoom -->
                    <div class="modal fade" id="zoomImg" tabindex="-1" aria-labelledby="zoomImgLabel" aria-hidden="true" style="z-index:1095">
                        <div class="modal-dialog modal-fullscreen">                          
                            <div class="modal-content">
                                <div class="modal-body">
                                    <button type="button" class="btn-close opacity-100 position-absolute end-0 fs-4 mx-4" data-bs-dismiss="modal" aria-label="Close" style="z-index:1100"></button>                                   
                                    <div class="item">
                                        <div class="swiper swiperZoom">
                                            <div class="swiper-wrapper">
                                                <div class="swiper-slide">
                                                    <picture>
                                                        <source srcset='https://statics.venca.com/web/img/products/112004_262073_PV_DF_FR_20230214101733_1_xl.jpg'>
                                                        <img src='https://statics.venca.com/web/img/products/112004_262073_PV_DF_FR_20230214101733_1_xl.webp' class='d-block img-fluid mx-auto' loading='lazy' title='' alt='' />
                                                    </picture>
                                                </div>
                                                <div class="swiper-slide">
                                                    <picture>
                                                        <source srcset='https://statics.venca.com/web/img/products/112004_262073_PV_DF_TL_20230214101733_2_xl.jpg'>
                                                        <img src='https://statics.venca.com/web/img/products/112004_262073_PV_DF_TL_20230214101733_2_xl.webp' class='d-block img-fluid mx-auto' loading='lazy' title='' alt='' />
                                                    </picture>
                                                </div>
                                                <div class="swiper-slide">
                                                    <picture>
                                                        <source srcset='https://statics.venca.com/web/img/products/112004_262073_PV_DF_BK_20230214101733_4_xl.jpg'>
