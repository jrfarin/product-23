<!-- ASIDE -->
<div class="col-12 col-md-5 col-lg-4">
    
    <!-- BREADCRUMBS -->
    <section id="breadcrumbs" class="row mb-3">
        {{> _Breadcrumbs }}
    </section>

    <!-- CHOOSE FEATURES -->
    <section id="chooseFeatures" class="row">
        {{> _ChooseFeatures }}
    </section>
</div>




