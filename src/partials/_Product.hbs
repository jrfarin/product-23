            </div>       
            <!-- Pagination Bullets -->
            <div class="position-relative" style="top:25px;">
                <div class="swiper-pagination"></div>
            </div>
        </div>

        <!-- Desktop Main Images -->
        <div id="galleryImages" class="col d-none d-md-block mb-3 mt-md-2">
            <div class="row flex-md-column flex-lg-row">
                <!-- Thumbs -->
                <div id="imgThumbs" class="col-md-12 col-lg-2 d-flex justify-content-between flex-lg-column order-md-2 order-lg-0 mt-md-2 mt-lg-0">                 
                    <picture>
                        <source srcset='https://statics.venca.com/web/img/products/112004_262073_PV_DF_FR_20230214101733_1_xs.jpg'>
